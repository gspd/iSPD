<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="183.0" prefWidth="480.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gspd.ispd.fxgui.simples.TerminalPaneController">
   <children>
      <VBox layoutX="215.0" layoutY="100.0" prefHeight="200.0" prefWidth="100.0" spacing="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <TextArea fx:id="textArea" editable="false" focusTraversable="false" prefHeight="200.0" prefWidth="200.0" wrapText="true" VBox.vgrow="ALWAYS">
               <font>
                  <Font name="Monospaced Regular" size="13.0" />
               </font></TextArea>
            <HBox prefHeight="0.0" prefWidth="480.0" spacing="5.0" VBox.vgrow="NEVER">
               <children>
                  <TextField fx:id="inputCommand" onKeyPressed="#handleKeyPressed" promptText="Command" HBox.hgrow="ALWAYS">
                     <font>
                        <Font name="Monospaced Regular" size="13.0" />
                     </font></TextField>
                  <Button mnemonicParsing="false" onAction="#handleOK" text="OK" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
